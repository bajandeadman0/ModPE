                 Entity.setRenderType(ant, 3); 
                 clientMessage("[YFS] AntVenom has spawned!");
                 break; } 
                 case'GoldSolace': {
                gold=(Level.spawnMob( Player.getX()-10,Player.getY(),Player.getZ() ,32,'mob/26.png' ));
                 gold;
                 Entity.setHealth(gold, 300);
                 Entity.setRenderType(gold, 3); 
                 clientMessage("[YFS] GoldSolace has spawned!");
                 break; } 
                 case'setosorcerer': {
                seto=(Level.spawnMob( Player.getX()-10,Player.getY(),Player.getZ() ,32,'mob/27.png' ));
                 seto;
                 Entity.setHealth(seto, 300);
                 Entity.setRenderType(seto, 3); 
                 clientMessage("[YFS] setosorcerer has spawned!");
                 break; } 
                 }
   } 
    
