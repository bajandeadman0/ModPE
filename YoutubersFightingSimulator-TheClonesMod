 var initialized=false;
 var ticks=7200;
var spawning = false;
var b =0;
var a =0;
var m = 0;
var skysword = 500;
var supersword = 500;
var betty =500;
var countdown =300;
 
function selectLevelHook() {
if(initialized==false) {
ModPE.setItem(470, "sword", 0, "Super Sword");
ModPE.setItem(471, "sword", 3, "Sky's budder Sword");
ModPE.setItem(472, "axe",4,"Betty");
initialized=true;
} 
}
  
 function procCmd(c) {
   var p = c.split(" ");
   var command = p[0];
   switch(command){
   default: {
            clientMessage("[YFS] Youtuber does not exist | not included | wrong spelling. Type /help for information!");
            break;
        } 
         case'durability':{
        clientMessage("[YFS] Betty's durability: "+betty+" \n[YFS] Sky's Budder Sword's durability: "+skysword+" \n[YFS] Super Sword's durability: "+supersword+" ");
        break;
        } 
        case 'killall': {
        Entity.setHealth(bajan,0);
         Entity.setHealth(bashur,0);
         Entity.setHealth(bowmaster,0);
         Entity.setHealth(sky,0);
         Entity.setHealth(deadlox,0);
         Entity.setHealth(husky,0);
         Entity.setHealth(bodil,0);
         Entity.setHealth(noah,0);
         Entity.setHealth(ian,0);
         Entity.setHealth(kermit,0);
         Entity.setHealth(seth,0);
         Entity.setHealth(bat,0);
         Entity.setHealth(mu,0);
         Entity.setHealth(me,0);
