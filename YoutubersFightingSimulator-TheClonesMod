 function leaveGame(){
              ModPE.saveData("betty",betty);
              ModPE.saveData("skysword",skysword);
              ModPE.saveData("supersword",supersword);
              ModPE.saveData("ticks",ticks); 
  
 }
 
function attackHook(attacker, victim){ 
if(getCarriedItem()==470)
{Entity.setHealth(victim, 1);
supersword--;
}
else if(getCarriedItem()==471){
Entity.setHealth(victim,Entity.getHealth(victim)-8)
skysword--;
}
 else if(getCarriedItem()==472){
Entity.setHealth(victim,Entity.getHealth(victim)-8)
betty--;
} 
if(victim==bajan|| victim==bashur|| victim==bowmaster|| victim==sky|| victim==deadlox|| victim==husky|| victim==bodil|| victim==noah|| victim==ian|| victim==kermit|| victim==seth|| victim==bat|| victim==mu|| victim==me|| victim==captain|| victim==bacca|| victim==lavamob|| victim==dawn|| victim==cupquake|| victim==harry|| victim==jerry|| victim==pocket|| victim==rush|| victim==vikk|| victim==notch|| victim==ant|| victim==gold|| victim==seto){ 
     Entity.setVelY(victim, -10.0);
     }
}
 
 function modTick() {
 ticks--;
 if(betty==0){
 Player.addItemInventory(472,-1);
 betty=500;
 }
 if(skysword==0){
 Player.addItemInventory(471,-1);
 skysword=500;
 }
 if(supersword==0){
 Player.addItemInventory(470,-1);
 supersword=500;
 } 
    if(ticks == 0) {
           a= Math.floor(Math.random() * 25) + 1;
        b= Math.floor(Math.random() * 25) + 1;
        m= Math.floor(Math.random() * 28)+1;
        spawning = true;
        ticks = 7200;   
        clientMessage("[YFS]Spawning...");
    }
    if(spawning == true) {
    if(m==1){
     bajan=(Level.spawnMob( Player.getX()-a,Player.getY(),Player.getZ()+b,32,'mob/1.png' ));
                 bajan;
                 Entity.setHealth(bajan, 300);
                 Entity.setRenderType(bajan, 3);
                 spawning=false;
                 }
                 else if(m==2){
