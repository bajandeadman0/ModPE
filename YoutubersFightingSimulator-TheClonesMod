 function procCmd(c) {
   var p = c.split(" ");
   var command = p[0];
   switch(command){
   default: {
            clientMessage("[YFS] Youtuber does not exist | not included | wrong spelling. Type /help for information!");
            break;
        } 
         case 'list': {
                 clientMessage("[YFS] Type /NameOfYoutuber to spawn a youtuber"); 
                    clientMessage("[YFS] [LIST] Youtubers List 1 of 4");
clientMessage("BajanCanadian");
 clientMessage("Bajan_Deadman0"); 
clientMessage("Bashur");
clientMessage("Bodil40");
 
 break; 
        }
         case 'list1': {
                 clientMessage("[YFS] Type /NameOfYoutuber to spawn a youtuber"); 
                    clientMessage("[YFS] [LIST] Youtubers List 1 of 4");
clientMessage("BajanCanadian");
clientMessage("Bajan_Deadman0");
clientMessage("Bashur");
clientMessage("Bodil40");
 break; 
