         Entity.setHealth(bacca,0);
         Entity.setHealth(lavamob,0);
         Entity.setHealth(dawn,0);
         Entity.setHealth(cupquake,0);
         Entity.setHealth(harry,0);
         Entity.setHealth(jerry,0); 
         Entity.setHealth(pocket,0);
         Entity.setHealth(rush,0);
         Entity.setHealth(vikk,0);
         Entity.setHealth(notch,0);
         Entity.setHealth(ant,0);
         Entity.setHealth(gold,0);
         Entity.setHealth(seto,0); 
          clientMessage("[YFS] Every Youtuber has been killed");
         break;
         }
        case 'superweapon': {
        Player.addItemInventory(470,1);
        clientMessage("[YFS] Superweapon has been given");
            break;
        }
         case 'list': {
            switch(p[1]) { 
            default: {
         clientMessage("[YFS] Type /NameOfYoutuber to spawn a youtuber"); 
                    clientMessage("[YFS] [LIST] Youtubers List 1 of 7");
 clientMessage("AntVenom"); 
clientMessage("BajanCanadian");
