 function procCmd(c) {
   var p = c.split(" ");
   var command = p[0];
   switch(command){
   default: {
            clientMessage("[YFS] Youtuber does not exist | not included | wrong spelling. Type /help for information!");
            break;
        } 
        case 'killall': {
        Entity.setHealth(bajan,0);
         Entity.setHealth(bashur,0);
         Entity.setHealth(bowmaster,0);
         Entity.setHealth(sky,0);
         Entity.setHealth(deadlox,0);
         Entity.setHealth(husky,0);
         Entity.setHealth(bodil,0);
         Entity.setHealth(noah,0);
         Entity.setHealth(ian,0);
         Entity.setHealth(kermit,0);
         Entity.setHealth(seth,0);
         Entity.setHealth(bat,0);
         Entity.setHealth(mu,0);
         Entity.setHealth(me,0);
         Entity.setHealth(captain,0);
         Entity.setHealth(bacca,0);
         Entity.setHealth(lavamob,0);
         Entity.setHealth(dawn,0);
         Entity.setHealth(cupquake,0);
         Entity.setHealth(harry,0);
         Entity.setHealth(jerry,0); 
         Entity.setHealth(pocket,0);
         Entity.setHealth(rush,0);
         Entity.setHealth(vikk,0);
         Entity.setHealth(notch,0);
          clientMessage("[YFS] Every Youtuber has been killed");
         break;
         }
        case 'superweapon': {
        Player.addItemInventory(268,1,-30000);
        clientMessage("[YFS] Superweapon has been given");
            break;
        }
         case 'list': {
            switch(p[1]) { 
            default: {
         clientMessage("[YFS] Type /NameOfYoutuber to spawn a youtuber"); 
                    clientMessage("[YFS] [LIST] Youtubers List 1 of 7");
clientMessage("BajanCanadian");
 clientMessage("Bajan_Deadman0"); 
clientMessage("Bashur");
clientMessage("Bodil40");
 break; 
        } 
          
         case '1':{
